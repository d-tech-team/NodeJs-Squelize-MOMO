                    <script>

                        const maintenanceWeb = `<%- setting.maintenance==='on' ? 'true' : 'false' %>`;

                        const token = $('meta[name="csrf-token"]').attr('content')

                        $(document).ready(function () {
                            $(document).on('click', '.fa-copy', function () {
                                let copyText = $(this).prev();
                                navigator.clipboard.writeText(copyText.text());
                                alert(document.title + ' đã sao chép: ' + copyText.text());
                            });

                            // Modal
                            window.onload = function () {
                                $('#tintuc').modal('show')
                            }

                            var title = new Typed('#title', {
                                strings: ['Hệ Thống Mini Game MoMo Tự Động'],
                                loop: true,
                                backDelay: 5000,
                                showCursor: false,
                            });
                            var subTitle = new Typed('#sub-title', {
                                strings: ['Uy Tín - Trao Thưởng 30s - Tự Động 24/7'],
                                loop: true,
                                backDelay: 5000,
                            });


                            // Diem danh
                            $('#btn-diem-danh').click(function (e) {
                                e.preventDefault();
                                let phone = $('#diem_danh_phone').val();
                                if (!phone) {
                                    return alert('Vui lòng nhập số điện thoại');
                                }
                                $('#btn-diem-danh').html('Đang kiểm tra').attr('disabled', true)
                                $.ajax({
                                    url: '/pointList',
                                    headers: {
                                        'CSRF-Token': token
                                    },
                                    type: 'POST',
                                    data: {
                                        phone
                                    },
                                    success: function (res) {
                                        const { msg } = res
                                        if (res.status) {
                                            setTimeout(() => {
                                                $('#btn-diem-danh').html('Điểm danh').attr('disabled', false)
                                                return $('#nhiemvungay').html(`<div class="alert alert-success" role="alert">
                            ${msg}</div>`)
                                            }, 1000);
                                            $('#diem_danh_phone').val('');
                                        }
                                        else {
                                            setTimeout(() => {
                                                $('#btn-diem-danh').html('Điểm danh').attr('disabled', false)
                                                return $('#nhiemvungay').html(`<div class="alert alert-warning" role="alert">
                            ${msg}</div>`)
                                            }, 1000);
                                        }
                                    }
                                });

                            });
                            $('#check-transaction').click(e => {
                                e.preventDefault()
                                const code = $('#magiaodich').val().trim()
                                const token = $('meta[name="csrf-token"]').attr('content')
                                if (!code) {
                                    alert('Vui lòng nhập mã giao dịch')
                                    return
                                }
                                const urlApi = '/checkCode'
                                $('#check-transaction').html('Đang kiểm tra').attr('disabled', true)

                                $.ajax({
                                    url: urlApi,
                                    headers: {
                                        'CSRF-Token': token
                                    },
                                    method: 'POST',
                                    data: {
                                        code
                                    },
                                    success: function (res) {
                                        const { status, msg } = res

                                        if (status) {
                                            setTimeout(() => {
                                                $('#check-transaction').html('Kiểm tra').attr('disabled', false)
                                                return $('#resultCheckMa').html(`<div class="alert alert-success" role="alert">
                            ${msg}</div>`)
                                            }, 3000);
                                        } else {
                                            setTimeout(() => {
                                                $('#check-transaction').html('Kiểm tra').attr('disabled', false)
                                                return $('#resultCheckMa').html(`<div class="alert alert-danger" role="alert">
                            ${msg}</div>`)
                                            }, 3000);
                                        }
                                    }
                                })

                            })
                        });
                        setInterval(() => {
                            const apiUrl = '/getHistoryWin'
                            $.ajax({
                                url: apiUrl,
                                method: 'GET',
                                success: function (res) {
                                    const { status, data } = res
                                    if (status) {
                                        let html = ''
                                        data.forEach(item => {
                                            html += `
                            <tr>
                                        <td>
                                            ${(item.transferPhone).substring(0, 7)}***
                                        </td>
                                        <td>
                                            ${(Intl.NumberFormat().format(item.amount)).replaceAll(',', '.')}
                                        </td>
                                        <td>
                                            ${(Intl.NumberFormat().format(item.amountReceive)).replaceAll(',', '.')}
                                        </td>
                                        <td>
                                            ${item.gameName}
                                        </td>
                                        <td>
                                            ${(item.comment).toUpperCase()}
                                        </td>
                                        <td><span class="badge badge-success text-uppercase">Thắng</span></td>
                                    </tr>
                            `
                                        })
                                        $('#load_data_play').html(html)
                                    }
                                }
                            })
                        }, 1000);
                    </script>

